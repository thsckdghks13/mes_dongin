{
  "main": "public/electron.js",
  "homepage": "./",
  "name": "mes_serom",
  "version": "1.4.5",
  "private": true,
  "description": "",
  "author":{
    "name":"이앤더블유",
    "email":"enw@naver.com"
  },  
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@mui/x-date-pickers": "^6.0.2",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "axios": "^1.3.4",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "electron-is-dev": "^2.0.0",
    "electron-progressbar": "^2.0.1",
    "electron-updater": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal-image": "^2.6.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "serialport": "^10.5.0",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "build:prod": "react-scripts build && electron-builder --win",
    "build:publish": "react-scripts build && electron-builder --win --publish always"
  },
  "build": {
    "appId": "com.mes_serom",
    "productName": "mes_serom",
    "directories": {
      "output": "mes_serom-build",
      "buildResources": "build-assets"
    },
    "win": {
      "publish": [
        "github"
      ],
      "icon": "./public/assets/icons/mes_icon.ico"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^22.2.0",
    "electron-builder": "^23.6.0"
  }
}
